{
  "name": "orionx-sdk",
  "version": "1.3.12",
  "description": "Help developers with Orionx integrations",
  "main": "build/index.js",
  "scripts": {
    "dev": "watch 'npm run build' src",
    "build": "babel  src -d build",
    "test": "ava --fail-fast --serial --verbose",
    "cover": "nyc npm run test"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "sources": [
      "./lib/*"
    ],
    "require": [
      "babel-register"
    ]
  },
  "keywords": [
    "orionx",
    "orion",
    "orionx-sdk"
  ],
  "author": "Orionx <contacto@orionx.com> (https://orionx.com/)",
  "homepage": "https://github.com/orionx-dev/orionx-sdk-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/orionx-dev/orionx-sdk-js/"
  },
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.26.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "babel-runtime": "6",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.9.2",
    "jssha": "^3.1.0",
    "node-fetch": "^2.6.0"
  }
}
