{
  "name": "orionx-sdk",
  "version": "1.3.15",
  "description": "Help developers with Orionx integrations",
  "main": "build/index.js",
  "scripts": {
    "dev": "watch 'npm run build' src",
    "build": "babel  src -d build",
    "test": "ava --fail-fast --serial --verbose",
    "cover": "nyc npm run test"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "ignoredByWatcher": [
      "**/node_modules/**",
      ".github/**"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "keywords": [
    "orionx",
    "orion",
    "orionx-sdk"
  ],
  "author": "Orionx <contacto@orionx.com> (https://orionx.com/)",
  "homepage": "https://github.com/orionx-dev/orionx-sdk-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/orionx-dev/orionx-sdk-js/"
  },
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.6.2",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.9.2",
    "jssha": "^3.1.0",
    "node-fetch": "^2.6.0",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "ava": "^4.2.0",
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "watch": "^0.13.0"
  }
}
